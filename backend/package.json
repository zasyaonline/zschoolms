{
  "name": "zschoolms-backend",
  "version": "1.0.0",
  "description": "ZSchool Management System Backend API with PostgreSQL",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "test": "echo \"Tests not yet implemented\" && exit 0",
    "lint": "eslint src/**/*.js",
